include "osfMESegments.fbs";

namespace ouster.OSFe;

table MLInference { 
    dataset_version:string;
    model_version:string;
    task_type:string;

    /// Usually a labeled osf will have all frame sources match
    /// the type here. It is possible for a frame to override
    /// the label type to allow an OSF to be a mixture of 
    /// predicted and ground truth labels.
    default_source_id:SEGMENT_SOURCE;
}

file_identifier "OMmi";
file_extension "osfmd";
root_type MLInference;
